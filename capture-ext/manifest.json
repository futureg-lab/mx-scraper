{
  "manifest_version": 3,
  "name": "mx-scraper-context-sender",
  "version": "1.0",
  "permissions": [
    "tabs",
    "scripting",
    "cookies",
    "activeTab"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Context Sender"
  }
}